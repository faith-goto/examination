<p id="notice"><%= notice %></p>

<h1>What Tests</h1>

<table>
  <thead>
    <tr>
      <th>Period</th>
      <th>Testname</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @what_tests.each do |what_test| %>
      <tr>
        <td><%= what_test.period %></td>
        <td><%= what_test.testname %></td>
        <td><%= link_to 'Show', what_test %></td>
        <td><%= link_to 'Edit', edit_what_test_path(what_test) %></td>
        <td><%= link_to 'Destroy', what_test, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New What Test', new_what_test_path %>
