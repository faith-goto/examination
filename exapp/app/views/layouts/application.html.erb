<!DOCTYPE html>
<html>
  <head>
    <title>App</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>
  <body>
    <header>
      <nav class="navbar navbar-expand-lg navbar-light bg-light">
        <%= link_to 'テストデータ一覧', test_results_path,{:class => "navbar-brand"} %>
        <div class="collapse navbar-collapse" id="Navber">
          <ul class="navbar-nav mr-auto">
            <li class="nav-item active">
              <%= link_to 'テストデータ一覧', test_results_path,{:class => "nav-link"} %>
            </li>
            <li class="nav-item">
              <%= link_to 'テスト種目一覧', what_tests_path,{:class => "nav-link"} %>
            </li>
            <li class="nav-item">
              <%= link_to '生徒一覧', users_path,{:class => "nav-link"} %>
            </li>
          </ul>
        </div>
      </nav>
    </header>
    <%= yield %>
  </body>
</html>
